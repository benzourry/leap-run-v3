{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js",
          "/fontawesome-webfont*"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "alpha-design-api-ui",
      "urls": [
        "https://io.reka.jdn.gov.my/api/app/**",
        "https://io.reka.jdn.gov.my/api/form/**",
        "https://io.reka.jdn.gov.my/api/dataset/**",
        "https://io.reka.jdn.gov.my/api/screen/**",
        "https://io.reka.jdn.gov.my/api/dashboard/**",
        "https://io.reka.jdn.gov.my/api/group/**",
        "https://io.reka.jdn.gov.my/api/mailer/**",
        "https://io.reka.jdn.gov.my/user/me"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "timeout": "10s",
        "maxSize": 100,
        "maxAge": "7d"
      }
    },{
      "name": "alpha-run-api-data",
      "urls": [
        "https://io.reka.jdn.gov.my/api/entry/**",
        "https://io.reka.jdn.gov.my/api/public/entry/**",
        "https://io.reka.jdn.gov.my/api/lookup/**",
        "https://io.reka.jdn.gov.my/api/public/lookup/**",
        "https://io.reka.jdn.gov.my/~/**"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "timeout": "10s",
        "maxSize": 120,
        "maxAge": "7d"
      }
    }
  ]
}